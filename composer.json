{
  "name":"lkochniss/gamer-profile",
  "description":"Show your gaming stats to the world",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "beta",
  "require": {
    "php": "^7.1.3",
    "aws/aws-sdk-php": "^3.90",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "doctrine/doctrine-migrations-bundle": "^1.3",
    "guzzlehttp/guzzle": "^6.3",
    "hybridauth/hybridauth": "^3.0@RC",
    "nelmio/cors-bundle": "^1.5",
    "symfony/asset": "^4.0",
    "symfony/browser-kit": "^4.0",
    "symfony/cache": "^4.0",
    "symfony/console": "^4.0@beta",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "^4.0",
    "symfony/security-csrf": "^4.0",
    "symfony/translation": "^4.0",
    "symfony/twig-bundle": "^4.0@RC",
    "symfony/validator": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "codacy/coverage": "^1.4",
    "nelmio/alice": "^3.1",
    "overtrue/phplint": "^1.1",
    "phploc/phploc": "^4.0",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11.8",
    "phpunit/phpunit": "^6.4",
    "sebastian/phpcpd": "^3.0",
    "squizlabs/php_codesniffer": "^3.2",
    "symfony/dotenv": "^4.0",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01BZ28CQ77YHE9N7RFSWPQ3D1Z",
      "allow-contrib": false
    }
  }
}
