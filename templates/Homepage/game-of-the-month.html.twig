{% extends 'base.html.twig' %}
{% block body %}
    <div class="text-right">
        {% for year in yearsWithSessions %}
            {% trans %}archive{% endtrans %}:
            <a href="{{ path('homepage_game_of_month_for_year', {year: year}) }}">{{ year }}</a>
        {% endfor %}
    </div>
    <h2 class="mb-3">{% trans from "messages" %}game_of_the_month{% endtrans %}</h2>
    {% include 'Homepage/_modules/game-of-the-month-list.twig' %}
{% endblock %}
